<odoo>
    <data>
        <record id="view_rfid_form" model="ir.ui.view">
            <field name="name">view_rfid_form</field>
            <field name="model">rfid.ser</field>
            <field name="arch" type="xml">
                <form string="RFID Serials" edit="false" create="false">
                    <sheet>
                        <div class="oe_title">
                            <h1>
                                <field name="lot_serial_no"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="product_id"/>
                                <field name="name"/>
                                <field name="status"/>
                            </group>
                            <group>
                                <field name="default_code"/>
                                <field name="barcode"/>
                                <field name="print_status"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_rfid_tree" model="ir.ui.view">
            <field name="name">view_rfid_tree</field>
            <field name="model">rfid.ser</field>
            <field name="arch" type="xml">
                <tree string="RFID Serials" create="false" edit="false">
                    <field name="product_id"/>
                    <field name="name"/>
                    <field name="lot_serial_no"/>
                    <field name="default_code"/>
                    <field name="barcode"/>
                    <field name="status"/>
                    <field name="print_status"/>
                </tree>
            </field>
        </record>

        <record id="action_rfid_view" model="ir.actions.act_window">
            <field name="name">RFID Serials</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">rfid.ser</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">

                </p>
                <p>

                </p>
            </field>
        </record>


        <menuitem id="menu_rfid_view" name="RFID Serials" parent="stock.menu_stock_inventory_control"
                  action="action_rfid_view" sequence="20"/>

        <record id="id_rfid_tagsinfo_tree" model="ir.ui.view">
            <field name="name">rfid tagsinfo tree</field>
            <field name="model">rfid.tagsinfo</field>
            <field name="arch" type="xml">
                <tree string="Rfid tagsinfo Tree">
                    <field name="state"/>
                    <field name="IDS"/>
                    <field name="DeviceID"/>
                    <field name="DeviceSerialNumber"/>
                    <field name="TagState"/>
                    <field name="EPC"/>
                    <field name="DeactvatedEPC"/>
                    <field name="ReactvatedEPC"/>
                    <field name="TID"/>
                    <field name="DataSource"/>
                    <field name="DecodedData"/>
                    <field name="TagSerialNumber"/>
                    <field name="DateTime"/>
                </tree>
            </field>
        </record>
        <record id="id_rfid_tagsinfo_form" model="ir.ui.view">
            <field name="name">rfid tagsinfo form</field>
            <field name="model">rfid.tagsinfo</field>
            <field name="arch" type="xml">
                <form string="Rfid tagsinfo Form">
                    <header>
                        <field name="state" widget="statusbar"
                               statusbar_visible="draft,sold,not_sold,returned,not_found"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field readonly="1" name="IDS"/>
                                <field readonly="1" name="DeviceID"/>
                                <field readonly="1" name="DeviceSerialNumber"/>
                                <field readonly="1" name="TagState"/>
                                <field readonly="1" name="EPC"/>
                                <field readonly="1" name="DeactvatedEPC"/>
                                <field readonly="1" name="ReactvatedEPC"/>
                            </group>
                            <group>
                                <field readonly="1" name="TID"/>
                                <field readonly="1" name="DataSource"/>
                                <field readonly="1" name="DecodedData"/>
                                <field readonly="1" name="TagSerialNumber"/>
                                <field readonly="1" name="DateTime"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="id_rfid_tagsinfo_search" model="ir.ui.view">
            <field name="name">rfid tagsinfo search</field>
            <field name="model">rfid.tagsinfo</field>
            <field name="arch" type="xml">
                <search string="Rfid tagsinfo Search">
                    <field name="DeviceSerialNumber"/>
                    <filter string="Not Found" name="state" domain="[('state','=','not_found')]"/>
                    <filter string="Archived" name="Archived" domain="[('active','=',True)]"/>
                    <filter string="Active" name="Active" domain="[('active','=',False)]"/>
                </search>
            </field>
        </record>
        <record id="id_rfid_tagsinfo_action" model="ir.actions.act_window">
            <field name="name">rfid tagsinfo action</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">rfid.tagsinfo</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{'search_default_Active': 1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p>
                <p>
                    <!-- More details about what a user can do with this object will be OK -->
                </p>
            </field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="id_rfid_tagsinfo_menu" name="RFID TagsInfo" parent="point_of_sale.menu_point_config_product"
                  action="id_rfid_tagsinfo_action"
                  sequence="1"/>

        <record id="res_config_settings_view_form_client_side" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.client.side</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='warehouse_setting_container']" position="after">
                    <h2 class="mt32">CLient Side URL</h2>
                    <div class="row mt16 o_settings_container" name="warehouse_setting_container">
                        <div class="col-12 col-lg-6 o_setting_box"
                             title="Store products in specific locations of your warehouse (e.g. bins, racks) and to track inventory accordingly.">
                            <div>
                                <label for="client_side_url"/>
                                <field name="client_side_url"/>
                                <p>For example http://0.0.0.0:8069</p>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>

        <record id="view_Tags_filter" model="ir.ui.view">
            <field name="name">tags.filter</field>
            <field name="model">tags.adjust</field>
            <field name="arch" type="xml">
                <search>
                    <field name="EPC"/>
                    <filter string="Archived" name="Archived" domain="[('active','=',True)]"/>
                    <filter string="Active" name="Active" domain="[('active','=',False)]"/>
                </search>
            </field>
        </record>

        <record id="id_Tags_act" model="ir.actions.act_window">
            <field name="name">Tags</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">tags.adjust</field>
            <field name="view_mode">tree</field>
            <field name="context">{'search_default_Active': 1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    <!-- Add Text Here -->
                </p>
                <p>
                    <!-- More details about what a user can do with this object will be OK -->
                </p>
            </field>
        </record>
        <menuitem id="UNIQUE_id_Tags_act" parent="stock.menu_stock_config_settings"
                  action="id_Tags_act" sequence="10"/>
    </data>
</odoo>